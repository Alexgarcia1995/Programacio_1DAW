package modules.user.classes.Order;
import java.util.Comparator;

import modules.user.classes.*;


public class Orderpoints implements Comparator <Persona>{
	
	public int compare(User u1, User u2) {
		
		if(((Normal)u1).getPoint() > ((User_register)u2).getPoint())
			return 1;
		if(((Normal)u1).getPoint() < ((User_register)u2).getPoint())
			return -1;
		return 0;
	}
}





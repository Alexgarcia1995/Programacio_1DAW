public class Prueba_Validadors {

	public static void main(String[] args) throws IOException {
		
       String PATH = new java.io.File(".").getCanonicalPath()+ "/src/modules/user/utils/admin/files/admin.json";

		 Gson gson = new Gson();
         JsonElement element = gson.toJsonTree(Singleton.useradmin, new TypeToken<ArrayList<Admin>>() {}.getType());
         JsonArray jsonArray = element.getAsJsonArray();
         
         String json = gson.toJson(jsonArray);

             FileWriter writer = new FileWriter(PATH);
             writer.write(json);
             writer.close();
}
}
		

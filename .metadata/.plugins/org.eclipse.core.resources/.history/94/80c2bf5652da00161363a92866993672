package modules.user.classes.Order;

import java.util.Comparator;

import modules.user.classes.*;

public class Ordershopping implements Comparator <User>{

	public int compare (User u1, User u2) {
		if(((Client)u1).getShopping()>((Client)u2).getShopping())
			return 1;
		if(((Client)u1).getShopping()<((Client)u2).getShopping())
			return -1;
		return 0;
	}
}



import javax.swing.JOptionPane;

import classes.Languages;
import modules.user.Model.BLL.BLL_dummies.Funciones_data_dummies;
import utils.formatos;
import utils.funciones;

public class Prueba_Validadors {
	public static void main(String[] args) {	
		int men=0, men2=0;
		String[] opciones={"Modificar config","Abrir","Guardar","Salir"};
		
		men2 = funciones.menu(opciones, "formato2", "formato");

		do{
			switch(men2){
			case 0:
				do {
					String[] option2= funciones.Menu4();
					men = funciones.menu(option2, "formato2", Languages.lenguajes.getProperty("formato"));
					switch (men) {
					case 0:
						formatos.Formatfecha();
						break;
					case 1:
						formatos.Formatomoneda();
						break;
					case 2:
						formatos.Formatdecimal();
						break;
					case 3:
						Languages.Lengua(Languages.lenguajes);
						break;
					case 4:
						formatos.Dummies();
						break;
					case 5:
						formatos.Formatfitx();
						break;
					case 6:
						formatos.FormatTheme();
						break;
					}
				} while (men != 7);
				break;
			case 1:
				break;
			case 2:
				break;
			}
			
		}while(men2!=3);
		
		
	}
}
		

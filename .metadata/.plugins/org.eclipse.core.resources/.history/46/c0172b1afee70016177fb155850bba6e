package classes;

import java.util.ArrayList;

public class Singleton_app {

	public static ArrayList<Settings> Setting;
	
	public static void create_settings(){
		Settings set = new Settings() ;
		Singleton_app.Setting.clear();
		set.setFormatfitx(Settings.Formatfitx);
		set.setLang(Settings.Lang);
		set.setCurrency_config2(Settings.currency_config);
		set.setDate_config2(Settings.date_config);
		set.setDecimals_config2(Settings.decimals_config);
		set.setDummies2(Settings.dummies);
		
		Singleton_app.Setting.add(set);
	}
}

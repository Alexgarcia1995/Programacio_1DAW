package classes;

import java.util.ArrayList;

public class Singleton_app {

	public static ArrayList<Settings> Settings;
	
	public static void create_settings(){
		Settings set
		
		set.setFormatfitx();
		set.setLang(lang);
		set.setCurrency_config(currency_configs);
		set.setDate_config();
		set.setDecimals_config(decimals_configs);
		set.setDummies(dummies);
		Singleton_app.Settings.add(set);
	}
}
